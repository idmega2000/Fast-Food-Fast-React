{
  "name": "yes",
  "version": "1.0.0",
  "description": "An online restaurant where users can order for food",
  "main": "app.js",
  "scripts": {
    "start": "npm run serve",
    "start:dev": "npm run build-dev",
    "build": "webpack --mode production",
    "serve": "npm run build && node server.js",
    "build-dev": "webpack-dev-server --mode development --open --hot",
    "test": "jest --coverage",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "linter": "eslint './**/*.js' './**/*.jsx'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run linter"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/src/test/mocks/imageMock.js",
      "\\.(css)$": "<rootDir>/src/test/mocks/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idmega2000/Fast-Food-Fast-React.git"
  },
  "author": "Idris Kelani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idmega2000/Fast-Food-Fast-React/issues"
  },
  "homepage": "https://github.com/idmega2000/Fast-Food-Fast-React#readme",
  "devDependencies": {
    "coveralls": "^3.0.2", 
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "moxios": "^0.4.0"

  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-jest": "^23.6.0",
    "express": "^4.16.4",
    "jest": "^24.0.0",
    "react-spinners": "^0.5.1",
    "react-toastify": "^4.5.2",
    "redux-devtools-extension": "^2.13.7",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "prop-types": "^15.6.2",
    "file-loader": "^3.0.1",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1"
  }
}
